.page {
  margin-top: 30rpx;
  margin-left: 5%;
  width: 90%;
}

.card {
  margin-bottom: 40rpx;
  display: flex;
  flex-direction: column;
  border: 1rpx solid #1177b0;
}

.titlecard {
  color: #fff;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  height: 90rpx;
  background-color: #1177b0;
}

.title {
  margin-left: 20rpx;
}

.icon_collection {
  position: absolute;
  right: 50rpx;
  height: 60rpx;
  width: 60rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.icon_comment {
  z-index: 20;
  position: absolute;
  right: 120rpx;
  height: 60rpx;
  width: 60rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}

.cardname {
  font-size: 32rpx;
  color: #81776e;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}

.listname {
  color: #000;
  margin-left: 50rpx;
  font-size: 28rpx;
}

.price {
  position: absolute;
  right: 10%;
  font-size: 24rpx;
  color: #f07c82;
}

.download {
  margin-top: 20rpx;
  width: 100%;
  background-color: #1177b0;
  border-radius: 0;
}

.download_hover {
  background-color: #11659a;
}

.nodatacss {
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  color: #898989;
}
